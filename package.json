{
  "name": "my-notes",
  "version": "0.1.0",
  "homepage": "https://tinnguyen263.github.io/my-notes-with-react",
  "dependencies": {
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "simplebar-react": "^0.1.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/ --ext .js ",
    "lint:fix": "eslint src/ --ext .js --fix",
    "test": "react-scripts test --passWithNoTests --no-watch",
    "test:watch": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "predeploy": "npm run lint && npm run test && npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1"
  }
}
